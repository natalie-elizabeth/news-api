{
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "node": true,
        "browser": true,
        "jest": true
    },
    "rules": {
        "one-var": 0,
        "comma-dangle": 0,
        "semi": 2
    }
}